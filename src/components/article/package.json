{
  "name": "react-article-components",
  "version": "0.1.9",
  "description": "Article Components for Rendering Article Page of The Reporter Site in Taiwan",
  "scripts": {
    "clean": "../../../node_modules/.bin/rimraf src",
    "build": "npm run clean && npm run build:js && npm run build:css",
    "build:js": "WEBPACK_CONFIG=$(pwd)/webpack.config.js BABEL_DISABLE_CACHE=1 BABEL_ENV=production NODE_ENV=production ../../../node_modules/.bin/babel --out-dir='src' --only='../../components/article/,../../lib/,../../constants/' ../../",
    "build:css": "mkdir -p src/styles && node ./concatCssFiles $(pwd) && ../../../node_modules/.bin/rimraf lib-css",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twreporter/twreporter-react"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twreporter/twreporter-react/issues"
  },
  "main": "src/components/article/index.js",
  "dependencies": {
    "classnames": "^2.2.1",
    "lodash": "^3.10.1",
    "raf": "^3.2.0",
    "rc-slider": "^3.7.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.0",
    "react-howler": "^3.0.1",
    "react-intl": "^2.1.2",
    "react-lazy-load": "^3.0.10",
    "react-responsive": "^1.1.3",
    "react-router": "^1.0.3"
  }
}
